                function getCookie2(variable) {
                  var query = document.cookie;
                  var vars = query.split(";");
                  for (var i = 0; i < vars.length; i++) {
                    vars[i] = vars[i].trim();
                    var pair = vars[i].split("=");
                    if (pair[0] == variable) {
                      if (pair[1].includes("?")) {
                        var pair1 = pair[1].split("?");
                        return pair1[0];
                      }
                      return pair[1];
                    }
                  }
                  return (false);
                }
